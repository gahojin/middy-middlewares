{
  "name": "@gahojin-inc/middy-appsync",
  "version": "2025.1.0",
  "description": "Middy appsync",
  "author": "GAHOJIN, Inc.",
  "license": "Apache-2.0",
  "type": "module",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/gahojin/middy-middlewares.git",
    "directory": "packages/middy-appsync"
  },
  "files": ["dist"],
  "keywords": ["middy", "appsync", "lambda", "aws"],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup",
    "prepublish": "pnpm run build",
    "lint": "biome check --write .",
    "check": "tsc --noEmit",
    "check:packagejson": "attw --pack .",
    "test": "vitest --watch",
    "test:unit": "vitest --passWithNoTests --run --coverage"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "devDependencies": {
    "@gahojin-inc/aws-lambda-mock-context": "2025.2.0",
    "@middy/core": "6.1.4",
    "@types/aws-lambda": "8.10.147"
  },
  "peerDependencies": {
    "@middy/core": "^5 || ^6"
  }
}
