{
  "name": "@gahojin-inc/middy-valibot-validator",
  "version": "2024.8.5",
  "description": "Middy valibot validator",
  "author": "GAHOJIN, Inc.",
  "license": "Apache-2.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/gahojin/middy-middlewares.git",
    "directory": "packages/middy-valibot-validator"
  },
  "files": ["dist"],
  "keywords": [],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup",
    "prepublish": "pnpm run build",
    "lint": "biome check --write .",
    "check": "tsc --noEmit",
    "test": "vitest --watch",
    "test:unit": "vitest --passWithNoTests --run --coverage"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "dependencies": {
    "valibot": "0.37.0"
  },
  "devDependencies": {
    "@middy/core": "5.4.6",
    "@types/aws-lambda": "8.10.143",
    "@valibot/i18n": "0.17.0",
    "aws-lambda-mock-context": "3.2.1"
  },
  "peerDependencies": {
    "valibot": "^0"
  }
}
