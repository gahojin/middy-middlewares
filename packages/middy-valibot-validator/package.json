    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "dependencies": {
    "@middy/core": "5.4.5",
    "@middy/util": "5.4.5",
    "valibot": "0.37.0"
  },
